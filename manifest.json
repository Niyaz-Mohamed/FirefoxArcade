{
    "manifest_version": 3,
    "name": "Firefox Arcade",
    "version": "0.1",
    "description": "Makes a custom startup page",
    "permissions": [
        "history",
        "storage",
        "activeTab"
    ],
    "chrome_url_overrides": {
        "newtab": "startup.html"
    },
    "icons": {
        "48": "images/icon-48.png"
    }
}